        },
        {
          "name": "new_folder",
          "placeholder": "请输入名称",
          "required": true,
          "type": "input-text",
          "editorState": "default",
          "label": "新目录"
        }
      ],
      "initiallyOpen": true,
      "deleteApi": {
        "method": "get",
        "url": "/api/v1/fs/${params.file_type}/folder/delete?folder=${folder}"
