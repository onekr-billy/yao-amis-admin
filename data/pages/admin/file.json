      "min-width": "250px",
      "width": "auto"
    }
  },
  "type": "page",
  "body": [
    {
      "type": "form",
      "id": "u:e7c4a9f53937",
      "mode": "horizontal",
      "dsType": "api",
      "title": "",
      "feat": "Insert",
      "target": "file_list",
      "body": [
        {
          "type": "input-text",
          "label": "",
          "labelClassName": "hidden",
          "name": "keywords",
          "id": "u:718499654773",
          "placeholder": "输入文件名",
          "addOn": {
            "type": "button",
            "label": "搜索",
            "onEvent": {
              "click": {
                "actions": [
                  {
                    "actionType": "submit",
                    "componentId": "u:e7c4a9f53937"
                  }
                ]
              }
            },
            "level": "primary",
            "id": "u:793cfc1a934f"
          }
        }
      ],
      "actions": []
    },
    {
      "id": "u:c000b24f40a8",
      "syncLocation": false,
      "api": {
        "method": "post",
        "trackExpression": "${file},${image},${folder}",
        "url": "/api/v1/fs/${params.file_type}/file/search?folder=${folder}&keywords=${keywords}"
      },
      "primaryField": "id",
      "multiple": true,
      "bulkActions": [
        {
          "level": "danger",
          "label": "批量删除",
          "actionType": "ajax",
          "confirmText": "确定要删除？",
          "api": {
            "url": "/api/v1/fs/${params.file_type}/file/bulk_delete",
            "method": "post",
            "data": {
              "items": {
                "${items}": {
                  "path": "${path}"
                }
              }
            }
          },
          "type": "button"
        }
      ],
      "headerToolbar": [
        "bulkActions",
        {
          "type": "button",
          "label": "上传文件",
          "actionType": "dialog",
          "level": "primary",
          "icon": "fas fa-cloud-upload-alt",
          "dialog": {
            "title": "上传文件",
            "body": [
              {
                "type": "form",
                "reload": "file_list",
                "body": [
                  {
                    "useChunk": false,
                    "labelClassName": "hidden",
                    "btnLabel": "文件上传",
                    "name": "file",
                    "receiver": {
                      "url": "/api/v1/fs/${params.file_type}/file/upload",
                      "data": {
                        "folder": "${folder}"
                      },
                      "dataType": "form-data",
                      "method": "post"
                    },
                    "drag": true,
                    "autoUpload": true,
                    "multiple": true,
                    "type": "input-file",
                    "accept": "",
                    "id": "u:ed4431f9629c"
                  },
                  {
                    "type": "input-image",
                    "hideUploadButton": false,
                    "imageClassName": "r w-full",
                    "proxy": true,
                    "uploadType": "fileReceptor",
                    "receiver": {
                      "url": "/api/v1/fs/${params.file_type}/file/upload",
                      "data": {
                        "folder": "${folder}"
                      },
                      "method": "post"
                    },
                    "visible": false,
                    "autoUpload": true,
                    "multiple": true,
                    "accept": ".jpeg, .jpg, .png, .gif",
                    "label": "",
                    "id": "u:4dab53d25ad7",
                    "limit": false,
                    "name": "image",
                    "editorState": "default"
                  }
                ]
              }
            ]
          }
        }
      ],
      "name": "file_list",
      "type": "crud",
      "perPage": 15,
      "columns": [
        {
          "label": "序号",
          "name": "id",
          "type": "number"
        },
        {
          "id": "u:c9edfda3cd3a",
          "label": "文件名",
          "name": "name",
          "type": "text",
          "sortable": true,
          "searchable": true
        },
        {
          "name": "path",
          "type": "text",
          "id": "u:c2da9234ea68",
          "label": "路径",
          "toggled": false
        },
        {
          "type": "text",
          "id": "u:aa1ba10a635b",
          "label": "大小",
          "name": "size",
          "sortable": true
        },
        {
          "id": "u:aa1ba10a635b",
          "label": "MIME类型",
          "name": "mime",
          "type": "text",
          "toggled": false
        },
        {
          "name": "type",
          "type": "text",
          "id": "u:aa1ba10a635b",
          "label": "文件类型",
          "sortable": true,
          "filterable": true
        },
        {
          "name": "time",
          "type": "text",
          "label": "修改时间",
          "sortable": true
        },
        {
          "label": "操作",
          "type": "operation",
          "buttons": [
            {
              "url": "/api/v1/fs/${params.file_type}/file/attachement?name=${path}",
              "actionType": "url",
              "className": "text-info",
              "id": "u:c345fb2a01fb",
              "label": "下载",
              "level": "link",
              "type": "button"
            },
            {
              "className": "text-info",
              "visible": false,
              "label": "下载-BLOB",
              "level": "link",
              "type": "button",
              "actionType": "download",
              "api": {
                "responseType": "blob",
                "url": "/api/v1/fs/${params.file_type}/file/attachement?name=${path}",
                "method": "get"
              }
            },
            {
              "confirmText": "确定要删除？",
              "id": "u:f2b43b894a2e",
              "label": "删除",
              "level": "link",
              "type": "button",
              "actionType": "ajax",
              "api": {
                "method": "get",
                "sendOn": "this.file_path",
                "url": "/api/v1/fs/${params.file_type}/file/delete?name=${path}"
              },
              "className": "text-danger"
            }
          ],
          "id": "u:835d380a797c"
        },
        {
          "label": "预览",
          "type": "operation",
          "buttons": [
            {
              "url": "/amis-admin/luckysheet.html?file=/api/v1/fs/${params.file_type}/file/attachement?name=${path}&token=${cookie:token}",
              "actionType": "url",
              "className": "text-info",
              "id": "u:c345fb2a01fb",
              "label": "预览",
              "level": "link",
              "type": "button",
              "visibleOn": "data.type === 'MS Excel'"
            },
            {
              "url": "/amis-admin/preview-excel.html?file=/api/v1/fs/${params.file_type}/file/attachement?name=${path}&token=${cookie:token}",
              "actionType": "url",
              "className": "text-info",
              "id": "u:c345fb2a01fb",
              "label": "预览",
              "level": "link",
              "type": "button",
              "visibleOn": "data.type === 'MS Excel'"
            },
            {
              "url": "/amis-admin/preview-pdf.html?file=/api/v1/fs/${params.file_type}/file/attachement?name=${path}&token=${cookie:token}",
              "actionType": "url",
              "className": "text-info",
              "id": "u:c345fb2a01fb",
              "label": "预览",
              "level": "link",
              "type": "button",
              "visibleOn": "data.type === 'PDF'"
            },
            {
              "url": "/PPTX2HTML/index.html?file=/api/v1/fs/${params.file_type}/file/attachement?name=${path}&token=${cookie:token}",
              "actionType": "url",
              "className": "text-info",
              "id": "u:c345fb2a01fb",
              "label": "预览",
              "level": "link",
              "type": "button",
              "visibleOn": "data.type === 'MS PPT'"
            },
            {
              "className": "text-info",
              "label": "预览",
              "level": "link",
              "type": "button",
              "actionType": "dialog",
              "dialog": {
                "title": "",
                "size": "lg",
                "body": {
                  "type": "office-viewer",
                  "src": "${url}",
                  "loading": true
                }
              },
              "visibleOn": "data.type === 'MS Word'"
            },
            {
              "url": "/amis-admin/preview-word.html?file=/api/v1/fs/${params.file_type}/file/attachement?name=${path}&token=${cookie:token}",
              "actionType": "url",
              "className": "text-info",
              "id": "u:c345fb2a01fb",
              "label": "预览",
              "level": "link",
              "type": "button",
              "visibleOn": "data.type === 'MS Word'"
            },
            {
              "type": "wrapper",
              "visibleOn": "data.type === 'Image'",
              "className": "flex flex-col-reverse content-center",
              "body": [
                {
                  "className": "text-info",
                  "id": "u:c345fb2a01fb",
                  "label": "预览",
                  "level": "link",
                  "type": "button",
                  "actionType": "dialog",
                  "dialog": {
                    "title": "",
                    "size": "md",
                    "body": {
                      "type": "image",
                      "imageMode": "original",
                      "src": "${url}",
                      "title": "${name}"
                    }
                  }
                },
                {
                  "label": "图片预览",
                  "src": "${url}",
                  "originalSrc": "${url}",
                  "type": "image",
                  "enlargeAble": true,
                  "className": "flex flex-nowrap justify-center"
                }
              ]
            },

            {
              "className": "text-info",
              "label": "播放",
              "level": "link",
              "type": "button",
              "actionType": "dialog",
              "dialog": {
                "bodyClassName": "p-0 m-0",
                "title": "",
                "size": "md",
                "body": {
                  "type": "video",
                  "src": "${url}"
                },
                "actions": []
              },
              "visibleOn": "data.type === 'Video'"
            },
            {
              "className": "text-info",
              "label": "播放",
              "level": "link",
              "type": "button",
              "actionType": "dialog",
              "dialog": {
                "title": "",
                "size": "md",
                "body": {
                  "type": "audio",
                  "src": "${url}",
                  "inline": false,
                  "rates": [1.0, 1.5, 2.0],
                  "controls": ["rates", "play", "time", "process", "volume"]
                },
                "actions": []
              },
              "visibleOn": "data.type == 'Audio'"
            }
          ]
        }
      ],
      "initFetchOn": "this.folder"
    }
  ],
  "title": "当前目录:${folder}",
  "asideSticky": false,
  "asideMinWidth": 250,
  "asideResizor": true,
