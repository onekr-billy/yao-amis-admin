    {
      "title": "",
      "type": "form",
      "actions": [],
      "body": [
        {
          "type": "tabs",
          "tabsMode": "chrome",
          "source": "${__code_sources}",
          "tabs": [
            {
              "title": "${title}",
              "body": [
                {
                  "type": "button-toolbar",
                  "buttons": [
                    {
                      "label": "复制Json源码",
                      "level": "primary",
                      "onEvent": {
                        "click": {
                          "actions": [
                            {
                              "actionType": "copy",
                              "args": {
                                "content": "${ENCODEJSON(${__code_source})}",
                                "copyFormat": "text/plain"
                              }
                            }
                          ]
                        }
                      },
                      "visibleOn": "this.language == 'json'",
                      "type": "button"
                    },
                    {
                      "label": "复制源码",
                      "level": "primary",
                      "onEvent": {
                        "click": {
                          "actions": [
                            {
                              "actionType": "copy",
                              "args": {
                                "content": "${__code_source}",
                                "copyFormat": "text/plain"
                              }
                            }
                          ]
                        }
                      },
                      "visibleOn": "this.language != 'json'",
                      "type": "button"
                    },
                    {
                      "type": "button",
                      "actionType": "dialog",
                      "level": "light",
                      "label": "预览",
                      "visibleOn": "${can_preview}",
                      "dialog": {
                        "size": "lg",
                        "body": [
                          {
                            "type": "amis",
                            "name": "__code_source"
                          }
                        ]
                      }
                    },
                    {
                      "type": "tpl",
                      "tpl": "${title}"
                    }
                  ]
                }
              ]
            }
          ],
          "onEvent": {
            "change": {
              "actions": [
                {
                  "actionType": "custom",
                  "script": "event.data.__code_sources && event.setData({...event.data,'__code_source':event.data.__code_sources[event.data.value -1]['__code_source']});"
                },
                {
                  "actionType": "setValue",
                  "args": {
                    "value": "${__code_source}"
                  },
                  "componentId": "code_editor"
                }
              ]
            }
          }
        },
        {
          "labelClassName": "hidden",
          "language": "${language || 'json'}",
          "name": "__code_source",
          "id": "code_editor",
          "type": "editor"
        }
      ],
      "onEvent": {
        "inited": {
          "actions": [
            {
              "actionType": "custom",
              "script": "event.data.__code_sources && event.data && event.setData({...event.data,'__code_source':event.data.__code_sources[0]['__code_source']});"
            },
            {
              "actionType": "setValue",
              "args": {
                "value": "${__code_source}"
              },
              "componentId": "code_editor"
            }
          ],
          "weight": 0
        }
      },
      "id": "u:078ef73682f4",
      "initApi": {
        "data": {
          "args": ["${model_id}", "${columns}"],
          "method": "${method}"
        },
        "method": "post",
        "url": "/api/__yao/app/service/schema"
      },
      "initFetch": false,
      "name": "schema_editor"
    }
  ],
  "id": "u:de916610a09e",
  "pullRefresh": {
    "disabled": true
  },
