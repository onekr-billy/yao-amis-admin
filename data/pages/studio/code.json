                                          },
                                          {
                                            "multiple": false,
                                            "type": "select",
                                            "name": "check_model_label",
                                            "label": "关联标签字段",
                                            "source": {
                                              "method": "post",
                                              "url": "/api/v1/system/model/ddic.model.column/select_options",
                                              "data": {
                                                "model_identity": "${check_model}",
                                                "_value": "name",
                                                "_label": "label"
                                              },
                                              "trackExpression": "${check_model}",
                                              "sendOn": "data.check_model"
