                                      {
                                        "body": [
                                          {
                                            "trueValue": 1,
                                            "type": "switch",
                                            "falseValue": 0,
                                            "label": "是否主键",
                                            "name": "primary"
                                          },
                                          {
                                            "trueValue": 1,
