            },
            {
              "type": "button",
              "label": "取消全选",
              "onEvent": {
                "click": {
                  "actions": [
                    {
                      "actionType": "custom",
                      "script": "event.data.columns.forEach(col => {col.checked = false})\n"
                    },
                    {
                      "componentId": "model_columns",
                      "actionType": "setValue",
                      "args": {
                        "value": "${event.data.columns}"
                      }
                    }
