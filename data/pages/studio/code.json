                                            "placeholder": "标签",
                                            "type": "input-text",
                                            "value": ""
                                          },
                                          {
                                            "columnClassName": "w-sm",
                                            "name": "value",
                                            "placeholder": "值",
                                            "type": "input-text",
                                            "value": ""
                                          }
                                        ],
                                        "label": "多选项",
                                        "multiple": true
                                      },
                                      {
                                        "type": "input-text",
                                        "default": "${label}",
                                        "id": "u:ff789489f74e",
                                        "label": "注释",
                                        "maxLength": 200,
                                        "name": "comment"
                                      },
                                      {
                                        "type": "combo",
                                        "name": "validations",
                                        "label": "检验",
                                        "multiple": true,
                                        "multiLine": false,
                                        "items": [
