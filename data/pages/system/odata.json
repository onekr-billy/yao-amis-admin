                      "searchable": true,
                      "sortable": true,
                      "type": "text"
                    },
                    {
                      "label": "关联模型",
                      "name": "model_id",
                      "quickEdit": {
                        "required": true,
                        "saveImmediately": true,
                        "source": {
                          "data": { "_label": "name", "_value": "identity" },
                          "method": "post",
                          "url": "/api/v1/system/model/ddic.model/select_options"
