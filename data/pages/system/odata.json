                      },
                      "searchable": true,
                      "sortable": true,
                      "type": "tag"
                    },
                    {
                      "label": "关联表格",
                      "name": "table_id",
                      "quickEdit": {
                        "saveImmediately": true,
                        "type": "input-text"
                      },
                      "searchable": true,
                      "sortable": true,
                      "type": "text"
                    },
                    {
                      "label": "可创建",
