      "link": "/model/model.create",
      "primary": true,
      "type": "button"
    },
    {
      "type": "button",
      "label": "从源码导入模型",
      "onEvent": {
        "click": {
          "actions": [
            {
              "actionType": "dialog",
              "dialog": {
                "size": "lg",
                "showErrorMsg": true,
                "showLoading": true,
                "className": "app-popover",
                "id": "u:b39aecfdb5fd",
                "type": "dialog",
                "title": "导入Yao模型代码",
                "body": [
                  {
                    "type": "form",
                    "api": "post:/api/v1/system/meta/model/import_source",
                    "body": [
                      {
                        "label": "模型标识",
                        "name": "ID",
                        "type": "input-text",
                        "required": true
                      },
                      {
                        "label": "模型源码",
                        "language": "json",
                        "id": "u:18805e926840",
                        "name": "source",
                        "type": "editor"
                      }
                    ],
                    "id": "u:297a88684e22",
                    "onEvent": {
                      "submitSucc": {
                        "actions": [
                          {
                            "actionType": "link",
                            "args": {
                              "link": "${`/model/create/` + result.data.id + `/`}"
                            }
                          }
                        ],
                        "weight": 0
                      }
                    }
                  }
                ],
                "showCloseButton": true
              }
            }
          ]
        }
      },
      "id": "u:40c005c0a0f1"
    },
    {
      "type": "button",
      "label": "导入系统模型",
      "onEvent": {
        "click": {
          "actions": [
            {
              "ignoreError": false,
              "outputVar": "",
              "actionType": "ajax",
              "options": {},
              "api": {
                "url": "/api/v1/system/meta/model/import_system_models",
                "method": "post"
              }
            },
            {
              "componentName": "model-list",
              "ignoreError": false,
              "actionType": "reload"
            }
          ]
        }
      },
      "id": "u:9bde38141acd"
    }
